<h1>Listing yeast_manufacturers</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @yeast_manufacturers.each do |yeast_manufacturer| %>
    <tr>
      <td><%= link_to yeast_manufacturer.name, yeast_manufacturer%></td>
      <td><%= link_to 'Edit', edit_yeast_manufacturer_path(yeast_manufacturer), :class => 'btn btn-mini' %></td>
      <td><%= link_to 'Destroy', yeast_manufacturer, method: :delete, data: { confirm: 'Are you sure?' }, :class => 'btn btn-mini btn-danger' %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<br />

<%= link_to 'New Yeast manufacturer', new_yeast_manufacturer_path %>
