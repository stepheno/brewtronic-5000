<h1>Yeasts</h1>
<div class="navbar">
  <div class="navbar-inner">
    <div class="container">
      <div class="nav-collapse">
        <%= form_tag yeasts_path, :method => 'get', :id => "yeasts_search", :class => "navbar-search pull-left" do %>
          <%= text_field_tag :search, params[:search], :class => "search-query", :placeholder => "Search" %>
        <% end %>
        <%= link_to raw('<i class="icon-plus-sign icon-white"></i> New Yeast'), new_yeast_path, :class => 'btn btn-primary pull-right'%>
      </div>
    </div>
  </div>
  <div id="yeasts"><%= render 'yeasts' %></div>
</div>
